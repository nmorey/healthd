# Data transfer counters: entries like hda, sda, md0, eth0
# example: USAGESTATS="hd;/dev/disk/by-id/myID;Label+for+graph hd;sda net;ifname;Label+for+if"
USAGESTATS=""

# Temperature values: internal name;sensor name;description+for+graph
# example: HEALTHSTATS="sensors;chip+name;sensors+string;Label+for+graph hd;sda;HDD0"
# Note: hd devices support any device path without the top /dev like disk/by-id/XXXX
HEALTHSTATS=""

# Raid to monitor
# example: MDSTATS="md1"
# Note: md devices support any device path without the top /dev.
MDSTATS=""

# UPS to monitor
# UPSSTATS="myups@localhost"
UPSSTATS=""

# List of docker images to monitor
# Set to '*' for all of them
DOCKERSTATS=""

# Email for temperature alert notifications
ADMIN_EMAIL=""

# Colors for graphs
COLORS="#E6194BC0;#3CB44BC0;#FFE119C0;#4363D8C0;#F58231C0;#911EB4C0;#42D4F4C0;#F032E6C0;#BFEF45C0;#FABED4C0;#469990C0;#DCBEFFC0;#9A6324C0;#FFFAC8C0;#800000C0;#AAFFC3C0;#808000C0;#FFD8B1C0;#000075C0;#A9A9A9C0"

# Web interface directory
WEB_DIR="@SHAREDSTATEDIR@/healthd/html/"

# Healthd RRD storage
RRD_DIR="@SHAREDSTATEDIR@/healthd/rrd/"

# Fallback max CPU temperature
# By default uses High value returned by sensors
CPU_TEMP_LIMIT=75

# Max disk temperature
DISK_TEMP_LIMIT=60

# Refresh period
REFRESH_PERIOD=60